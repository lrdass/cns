{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "DAP clojurescript",
      "url": "http://localhost:8080",
      "sourceMaps": true,
      "webRoot": "${workspaceFolder}",
      "sourceMapPathOverrides": {
        "*": "${workspaceFolder}/src/main/*"
      }
    }
  ]
}
